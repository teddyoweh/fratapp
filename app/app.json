{
  "expo": {
    "name": "HERDS",
    "slug": "herds",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/HERDS-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#111111"
   
      
      
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "platforms": [
      "ios"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teddyoweh.herds"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/HERDS.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/HERDS-icon.png",
          "color": "#000000"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c962b3bb-afa1-42df-94c2-b8461f2d6019"
      }
    }
  }
}
